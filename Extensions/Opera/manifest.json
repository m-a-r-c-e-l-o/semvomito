{
   "author": "m-a-r-c-el-o",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "cura.js" ],
      "matches": [ "*://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Fique livre dessa doença, remova os vômitos do seu facebook.",
   "developer": {
      "name": "m-a-r-c-e-l-o"
   },
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "64": "icon64.png"
   },
   "manifest_version": 2,
   "name": "Anti-Vômitaço",
   "permissions": [ "*://*.facebook.com/*", "*://*.fbcdn.net/*" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "1.0.1"
}
