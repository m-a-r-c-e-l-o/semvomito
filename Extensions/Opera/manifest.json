{
   "author": "m-a-r-c-el-o",
   "background": {
      "scripts": [ "background.js" ]
   },
   "permissions": ["tabs"],
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "cura.js" ],
      "matches": [ "*://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Fique livre dessa doença, remova os vômitos do seu facebook.",
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
      "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Anti Vômitaço",
   "version": "1"
}
