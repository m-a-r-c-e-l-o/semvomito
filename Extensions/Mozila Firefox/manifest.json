{
   "author": "m-a-r-c-el-o",

   "permissions": ["*://*.facebook.com/*",
      "*://*.fbcdn.net/*"],
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "index.js" ],
      "matches": [ "*://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Fique livre dessa doença, remova os vômitos do seu facebook.",
      "icons": {
      "128": "icon128.png",
      "64": "icon64.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Anti Vômitaço",
   "version": "1.0.3"
}
